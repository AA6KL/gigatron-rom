                              ShiftRight1bit                    
                              STW                               _mask                           ; mask = mask LSR 1

                              LDWI                              _while_0x0379
                              CALL                              giga_vAC                        ; wend

_wend_0x03e0                  Return                                                            ; return

_end_0x03e2                   BRA                               _end_0x03e2                     ; END



