'
' Setup sound channels
'
10 for c=1 to 4
11 p=c*256+252
12 poke p-1,2:'Pulse
13 poke p,0:poke p+1,0
14 next c
'
' Setup vCPU note lookup
'
20 poke &h78,17:'LDWI $9xx
21 poke &h7a,9
22 poke &h7b,127:'LUP 0
23 poke &h7c,0
24 poke &h7d,147:'INC &h79
25 poke &h7e,&h79
26 poke &h7f,255:'RET
'
' Play all notes
'
30 for n=1 to 95
31 print "Playing note",n
32 poke &h79,n+n:'$900+n*2
33 poke p,  usr(&h78):'Flo
34 poke p+1,usr(&h78):'Fhi
35 poke 44,15:'0.25 secs
36 if peek(44)>0 then goto 36
37 next n
