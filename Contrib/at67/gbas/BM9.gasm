                    LDW                     _K
                    STW                     mathY
                    LDWI                    divide16bit
                    CALL                    giga_vAC
                    STW                     _L                              ; L=N / K

_150                LDW                     _L
                    CALL                    convertEqOpAddr
                    BEQ                     _160                
                    BRA                     _200                            ; IF L=0 THEN GOTO 200

_160                LDW                     _L
                    SUBI                    1
                    CALL                    convertEqOpAddr
                    BEQ                     _170                
                    BRA                     _190                            ; IF L=1 THEN GOTO 190

_170                LDW                     _M
