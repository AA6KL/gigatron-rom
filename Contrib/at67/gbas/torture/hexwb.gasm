; Labels
_entryPoint_        EQU                     0x0200
_end_0x031a         EQU                     0x08c8
_end_0x031c         EQU                     0x08ca
_next_0x025b        EQU                     0x025b
_page_0x08a0        EQU                     0x08a0

; Variables
_z                  EQU                     0x0030
_i                  EQU                     0x0032
_a                  EQU                     0x0034
_b                  EQU                     0x0036
_c                  EQU                     0x0038
_d                  EQU                     0x003a
_e                  EQU                     0x003c

; Strings
usrStr_0x7ff9       EQU                     0x7ff9
usrStr_0x7ff9       DB                      1 ' '

; Code
_entryPoint_        Initialise              
                    InitialiseCcOps                                         ; INIT

                    LDI                     3
                    STW                     _z                              ; z = 3

                    LDI                     0
                    STW                     _i                              ; for i=0 to 5

_next_0x025b        LDI                     16
                    STW                     _a                              ; a = &h10

                    LDW                     _a
                    ADDI                    100
                    STW                     _b                              ; b = a + 100

                    LDWI                    -1
                    SUBW                    _a
                    SUBW                    _b
                    CALL                    convertNeOpAddr
                    STW                     _c
                    PEEK                    
                    STW                     0xd2
                    LDW                     _c
                    STW                     _d                              ; c = NOT a <> b : d = peek(c)

                    LDWI                    0xaa55
                    XORW                    _a
                    ORW                     _c
                    STW                     0xd0
                    LDWI                    0xffff
                    ANDW                    0xd0
                    STW                     _e                              ; e = &hAA55 XOR a OR c AND &hFFFF

                    LDW                     _z
                    ADDI                    1
                    STW                     _z                              ; z = z + 1

                    PrintVarInt16           _a
                    PrintString             usrStr_0x7ff9
                    PrintVarInt16           _b
                    PrintString             usrStr_0x7ff9
                    PrintVarInt16           _c
                    PrintString             usrStr_0x7ff9
                    LDW                     _d
                    PrintAcHexByte          
                    PrintString             usrStr_0x7ff9
                    LDW                     _e
                    PrintAcHexWord          
                    STW                     0xe0
                    LDWI                    _page_0x08a0
                    CALL                    giga_vAC
_page_0x08a0        LDW                     0xe0
                    PrintString             usrStr_0x7ff9
                    PrintVarInt16           _z
                    LDWI                    newLineScroll
                    CALL                    giga_vAC
                    ForNextLoopUp           _i _next_0x025b 5               ; print a;" ";b;" ";c;" ";hex$(d);" ";hexw$(e);" ";z : next i

_end_0x031a         BRA                     _end_0x031a                     ; END

_end_0x031c         BRA                     _end_0x031c                     ; END


