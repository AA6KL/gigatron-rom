                    LDI                     0
                    STW                     _x
                    LDI                     8
                    STW                     _v                              ; x=0 : v=8

_loop               LDW                     _x
                    ANDI                    255
                    STW                     _i                              ; i = x AND 255

                    LDI                     0
                    STW                     _j                              ; j = 0

                    STW                     0xde
                    LDWI                    _page_0x0400
                    CALL                    giga_vAC
_page_0x0400        LDW                     0xde
                    LDWI                    496
                    STW                     _q                              ; q = &h0100 + 240

                    LDI                     32
                    STW                     _yy                             ; yy = horizonY

                    LDWI                    _calcPitch
                    CALL                    giga_vAC                        ; gosub calcPitch

                    LDI                     0
                    STW                     _y
                    LDW                     _pitch
                    STW                     0xb0
                    LDI                     1
                    STW                     0xb2                            ; for y=0 &to pitch

_next_0x046b        LDW                     _j
                    ADDW                    _i
                    STW                     _j                              ; j = j + i

                    ForNextVarPos           _y _next_0x046b 0xb0 0xb2       ; next y

                    LDW                     _i
                    ADDW                    _i
                    STW                     _i                              ; i = i + i

                    LDWI                    321
                    STW                     _p                              ; p = &h0101 + horizonY + horizonY

                    WaitVBlank                                              ; wait

_repeat_0x0490      LD                      _j + 1
                    POKE                    _p                              ; poke p, j LSR 8

                    LDW                     _j
                    ANDI                    255
                    ADDW                    _i
                    STW                     _j                              ; j = i + (j AND 255)

                    LDW                     _p
                    ADDI                    4
                    STW                     _p
                    SUBW                    _q
                    BLT                     _repeat_0x0490                  ; p = p + 4 : until p &&>= q

                    LD                      giga_buttonState
                    STW                     numericLabel
                    LDI                     0
                    STW                     defaultLabel
                    GosubNumeric                                            ; gosub get("BUTTONSTATE")

                    LDW                     _x
                    ADDW                    _v
                    STW                     _x                              ; x = x + v

                    LDWI                    _loop
                    CALL                    giga_vAC                        ; goto loop

_254                PUSH                    
                    LDW                     _v
                    ADDI                    1
                    STW                     _v
                    SUBI                    32
                    BLE                     _else_0x04ef
                    LDI                     32
                    STW                     _v                              ; v = v + 1 : if v &&> 32 then v = 32

_else_0x04ef        Return                                                  ; return

_253                PUSH                    
                    LDW                     _v
                    SUBI                    1
                    STW                     _v
                    LDWI                    -32
                    SUBW                    _v
                    BLT                     _else_0x050b
                    LDWI                    -32
                    STW                     _v                              ; v = v - 1 : if v &&< -32 then v = -32

_else_0x050b        Return                                                  ; return

_191                PUSH                    
                    LDI                     0
                    STW                     _v
                    Return                                                  ; v = 0 : return

_127                PUSH                    
                    LDI                     0
                    STW                     _v
                    Return                                                  ; v = 0 : return

_calcPitch          PUSH                    
                    LDWI                    -23
                    ADDW                    _yy
                    STW                     _pitch                          ; pitch = yy + (96 - 120 + 1)

                    LDW                     _pitch
                    ADDI                    1
                    STW                     0xc0
                    LDWI                    0xfffe
                    ANDW                    0xc0
                    STW                     _pitch                          ; pitch = (pitch + 1) AND &hfffe

                    Return                                                  ; return

_end_0x0536         BRA                     _end_0x0536                     ; END



