; Labels
_entryPoint_        EQU                     0x0200
_1                  EQU                     0x0253
_10                 EQU                     0x0253
_20                 EQU                     0x0257
_30                 EQU                     0x026a
_40                 EQU                     0x027a
_50                 EQU                     0x0293
_end_0x02ac         EQU                     0x02a6
_next_0x0257        EQU                     0x0257
_next_0x0280        EQU                     0x027a

; Variables
_Y                  EQU                     0x0030
_D                  EQU                     0x0032
_E                  EQU                     0x0034
_C                  EQU                     0x0036

; Strings

; Code
_entryPoint_        Initialise              
                    InitialiseCcOps                                         ; INIT

_10                 LDI                     2
                    STW                     _Y                              ; FOR Y=2 TO 7

_20                 LDW                     _Y
                    STW                     mathX
                    LDI                     16
                    STW                     mathY
                    LDWI                    multiply16bit
                    CALL                    giga_vAC
                    STW                     _D
                    ADDI                    15
                    STW                     _E                              ; D=Y*16: E=D+15

_30                 LDI                     32
                    ST                      cursorXY
                    LDW                     _D
                    STW                     _C
                    LDW                     _E
                    STW                     0xc4
                    LDI                     1
                    STW                     0xc6                            ; AT 32: FOR C=D TO E

_40                 LDW                     _C
                    PrintAcChar             
                    ForNextLoopStepUp       _C _40                  0xc4 0xc6                                                                           ; PUT C: NEXT C

_50                 LDWI                    newLineScroll
                    CALL                    giga_vAC
                    ForNextLoopUp           _Y _20                  7       ; PRINT: NEXT Y

_end_0x02ac         BRA                     _end_0x02ac                     ; END


