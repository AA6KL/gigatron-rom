_z              EQU		0x003e

; Strings
usrStr_0x6fa0		EQU		0x6fa0
usrStr_0x6fa0		DB		1 ' '
usrStr_0x6fa2		EQU		0x6fa2
usrStr_0x6fa2		DB		7 ' 123   '

; Code
_entryPoint_	Initialise			; INIT

_init           LDI		1
                STW		_a		; a=1

                STW		_b		; b=a

_32453245324543 LDWI	-3
                STW		_cat		; cat=-3

_10             PrintVarInt16	_a
                PrintString	usrStr_0x6fa0
                PrintVarInt16	_z
                PrintString	usrStr_0x6fa0
                PrintVarInt16	_f
                PrintString	usrStr_0x6fa2
                CALL	newLineScroll		; printa;" ";z;" ";f;" 123   "

                LDW		_d
                ADDI	5
                STW		0x82
                LDI		20
                ADDW	0x82
                STW		0x82
                LDW		_cat
                SUBW	_a
                STW		0x84
                LDW		0x82
                ADDW	0x84
                STW		_d		; d=20+(d+5)+(cat-a)

                LDW		_b
                SUBI	3
                STW		0x82
                LDI		25
                SUBW	_a
                STW		0x84
                LDW		0x82
                ADDW	0x84
                ADDW	_d
                SUBW	_b
                STW		0x84
                LDW		_a
                SUBW	_cat
                SUBW	_d
                STW		0x86
                LDW		0x84
                SUBW	0x86
                STW		_e		; e=b-3+(25-a)+d-b-(a-cat-d)

                LDI		64
                SUBW	_e
                STW		_f		; f=54-+23+(27*2)-32+(56/3)-7-e

                LDWI	-1149
                STW		_g		; g=154-1230+(227*2)-532+(756/33)-17

                LDW		_a
                ADDI	1
                STW		_a		; a=a+1

                LDW		_b
                SUBI	1
                STW		_b		; b=b-1

                LDWI	-2949
                ADDW	_b
                STW		0x82
                LDWI	300
                ADDW	_e
                STW		0x84
                LDWI	-900
                ADDW	_f
                STW		0x86
                LDW		0x84
                SUBW	0x86
                STW		0x86
                LDW		0x82
                SUBW	0x86
                STW		_z		; z=b-(1000+2000-53+2)-(e+300-(f-900))

_31             LDI		0
                SUBW	_g
                STW		0x82
                LDWI	1000
                ADDW	0x82
                STW		0x82
                LDI		24
                SUBW	_g
                STW		0x84
                LDW		0x82
                SUBW	0x84
                ADDW	_f
                STW		_f		; f=-g+1000-(24-g)+f

_40             CALL	_10		; goto10

